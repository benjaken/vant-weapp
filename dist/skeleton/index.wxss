@import '../common/index.wxss';.van-skeleton{height:100%!important;left:0;overflow:hidden;top:0;width:100%!important;z-index:999}.van-skeleton,.van-skeleton__item{position:absolute}.van-skeleton__item--bg{background-color:#fff}.van-skeleton__item--rect{background-color:#969799;border-radius:4px;opacity:.2}.van-skeleton__item--circle{background-color:#969799;border-radius:999px;opacity:.2}.van-skeleton__spinbox{align-items:center;display:none;height:100%;justify-content:center;position:absolute;width:100%;z-index:9999}.van-skeleton__spin{display:inline-block;height:64rpx;width:64rpx}.van-skeleton__spin:after{animation:spin 1.2s linear infinite;border-color:#307c8a transparent;border-radius:999px;border-style:solid;border-width:5rpx;content:" ";display:block;height:46rpx;margin:1rpx;width:46rpx}.van-skeleton__render-button{background-color:#307c8a;border-radius:999px;bottom:5px;box-shadow:2px 2px 6px rgba(134,151,197,.17);color:#fff;font-size:24rpx;height:45px;line-height:45px;position:fixed;right:5px;text-align:center;width:45px;z-index:9999}.van-skeleton__error{align-items:center;display:flex;flex-direction:column;height:60vh;justify-content:center}.van-skeleton__error-text{color:#969799;font-size:14px;margin:12px 0}.van-skeleton__error-button{width:160rpx}@keyframes spin{0%{transform:rotate(0deg)}to{transform:rotate(1turn)}}@keyframes flush{0%{transform:translate(-100%,-100%) rotate(45deg)}to{transform:translate(100%,100%) rotate(45deg)}}.van-skeleton__shine{animation:flush 2s linear infinite;background:linear-gradient(90deg,hsla(0,0%,100%,0) 10%,hsla(0,0%,100%,.85) 50%,hsla(0,0%,100%,0) 90%);display:none;height:200%;position:absolute;top:-50%;width:500px}.van-skeleton--shine .van-skeleton__shine{display:block}@keyframes chiaroscuro{0%{opacity:.7}50%{opacity:1}to{opacity:.7}}.van-skeleton--chiaroscuro .van-skeleton__item{animation-duration:2s;animation-iteration-count:infinite;animation-name:chiaroscuro}