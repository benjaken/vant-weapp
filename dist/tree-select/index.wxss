@import '../common/index.wxss';.van-tree-select{display:flex;font-size:var(--tree-select-font-size,14px);position:relative;-webkit-user-select:none;user-select:none}.van-tree-select__nav{--sidebar-padding:12px 8px 12px 12px;background-color:var(--tree-select-nav-background-color-light,#f7f8fa);flex:1}.van-tree-select__nav__inner{height:100%;width:100%!important}.van-tree-select__content{background-color:var(--tree-select-content-background-color-light,#fff);flex:2}.van-tree-select__item{font-weight:700;line-height:var(--tree-select-item-height,44px);padding:0 32px 0 var(--padding-md,16px);position:relative}.van-tree-select__item--active{color:var(--tree-select-item-active-color,#f96401)}.van-tree-select__item--disabled{color:var(--tree-select-item-disabled-color-light,#c8c9cc)}.van-tree-select__selected{position:absolute;right:var(--padding-md,16px);top:50%;transform:translateY(-50%)}@media (prefers-color-scheme:dark){.van-tree-select__nav{background-color:var(--tree-select-nav-background-color-dark,#000)}.van-tree-select__content{background-color:var(--tree-select-content-background-color-dark,#1c1c1e)}.van-tree-select__item--disabled{color:var(--tree-select-item-disabled-color-dark,#4d4d4d)}}